# Warning in wave window restart: (vish-4014) No objects found matching '/addrgen2/tmp_addr'. 
# Warning in wave window restart: (vish-4014) No objects found matching '/addrgen2/tmp_addr'. 
add wave \
{sim:/addrgen2/state } \
{sim:/addrgen2/nextstate } \
{sim:/addrgen2/resend } \
{sim:/addrgen2/clk } \
{sim:/addrgen2/rst } \
{sim:/addrgen2/r_enable_in } \
{sim:/addrgen2/w_enable_in } \
{sim:/addrgen2/read_ready } \
{sim:/addrgen2/r_enable_out } \
{sim:/addrgen2/w_enable_out } \
{sim:/addrgen2/read_done } \
{sim:/addrgen2/counter32 } \
{sim:/addrgen2/newcounter32 } \
{sim:/addrgen2/addr } \
{sim:/addrgen2/addr_counter } \
{sim:/addrgen2/new_addr } \
{sim:/addrgen2/store_addr } \
{sim:/addrgen2/new_store } 
force read_ready 0 0 ns, 1 50 ns
force Resend 0 0 ns
force r_enable_in 0 0 ns, 1 55 ns
force w_enable_in 0 0 ns, 1 5 ns, 0 50 ns
force rst 1 0 ns, 0 1.75 ns
force clk 0 0 ns, 1 1.75 ns -r 3.5 ns
run 300 ns
rm -rf work
vlib work
vcom source/ReadController.vhd
