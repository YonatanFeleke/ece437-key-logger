vlib work
vcom source/AddressGenerator.vhd
# Model Technology ModelSim SE-64 vcom 6.6b Compiler 2010.05 May 21 2010
# -- Loading package standard
# -- Loading package std_logic_1164
# -- Loading package std_logic_arith
# -- Loading package std_logic_unsigned
# -- Compiling entity addressgenerator
# -- Compiling architecture statemachine of addressgenerator
# ** Error: source/AddressGenerator.vhd(101): near "r_enable_out": expecting ';'
# ** Error: source/AddressGenerator.vhd(109): VHDL Compiler exiting
# /package/eda/mg/modeltech_6.6b/modeltech/linux_x86_64/vcom failed.
vcom source/AddressGenerator.vhd
# Model Technology ModelSim SE-64 vcom 6.6b Compiler 2010.05 May 21 2010
# -- Loading package standard
# -- Loading package std_logic_1164
# -- Loading package std_logic_arith
# -- Loading package std_logic_unsigned
# -- Compiling entity addressgenerator
# -- Compiling architecture statemachine of addressgenerator
vsim -i work.addressgenerator
# vsim -i work.addressgenerator 
# ** Note: (vsim-3812) Design is being optimized...
# Loading std.standard
# Loading ieee.std_logic_1164(body)
# Loading ieee.std_logic_arith(body)
# Loading ieee.std_logic_unsigned(body)
# Loading work.addressgenerator(statemachine)#1
add wave \
{sim:/addressgenerator/state } \
{sim:/addressgenerator/nextstate } \
{sim:/addressgenerator/resend } \
{sim:/addressgenerator/clk } \
{sim:/addressgenerator/rst } \
{sim:/addressgenerator/r_enable_in } \
{sim:/addressgenerator/w_enable_in } \
{sim:/addressgenerator/num_writes } \
{sim:/addressgenerator/empty } \
{sim:/addressgenerator/r_enable_out } \
{sim:/addressgenerator/counter32 } \
{sim:/addressgenerator/newcounter32 } \
{sim:/addressgenerator/addr } \
{sim:/addressgenerator/addr_counter } \
{sim:/addressgenerator/new_addr } \
{sim:/addressgenerator/store_addr } \
{sim:/addressgenerator/new_store } 
force rst 1 0 ns, 0 1 1.75 ns
# ** Error: (vsim-4012) Expected ',' separator between value/time pairs.
# Usage: force [-deposit | -drive | -freeze] [-cancel <period>] [-repeat <period>] <item_name> <value> [<time>] [, <value> <time> ...]
force rst 1 0 ns, 1 1.75 ns
force clk 0 0 ns, 1 1.75 ns -r 3.5 ns
force r_enable_in 0 0 ns, 1 5 ns
force w_enable_in 0 0 ns
force num_writes 0 0 ns
run 500 ns
restart
force num_writes 0 0 ns
force w_enable_in 0 0 ns, 1 5 ns
force r_enable_in 0 0 ns
force clk 0 0 ns, 1 1.75 ns -r 3.5 ns
force rst 1 0 ns, 1 1.75 ns
run 500 ns
restart]
# invalid command name "restart]"
restart
force rst 1 0 ns, 0 1.75 ns
force clk 0 0 ns, 1 1.75 ns -r 3.5 ns
force r_enable_in 0 0 ns
force w_enable_in 0 0 ns, 1 5 ns
force num_writes 0 0 ns
force Resend 0 0 ns
run 500 ns
rm -rf work
vlib work
