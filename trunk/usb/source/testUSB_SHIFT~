rm -rf work
vlib work
#vcom source/USB_SHIFT.vhd
vcom mapped/USB_SHIFT.vhd
vcom source/tb_USB_SHIFT.vhd
vsim -i work.tb_usb_shift

add wave \
{sim:/tb_usb_shift/clk } \
{sim:/tb_usb_shift/rst } \
{sim:/tb_usb_shift/eop } \
{sim:/tb_usb_shift/shift_enable } \
{sim:/tb_usb_shift/d_orig } \
{sim:/tb_usb_shift/rcv_data } \
{sim:/tb_usb_shift/crc_shift } \
{sim:/tb_usb_shift/stuff_error } 
add wave \
{sim:/tb_usb_shift/dut/rcv_data_7_port } \
{sim:/tb_usb_shift/dut/rcv_data_6_port } \
{sim:/tb_usb_shift/dut/rcv_data_5_port } \
{sim:/tb_usb_shift/dut/rcv_data_4_port } \
{sim:/tb_usb_shift/dut/rcv_data_3_port } \
{sim:/tb_usb_shift/dut/rcv_data_2_port } \
{sim:/tb_usb_shift/dut/rcv_data_1_port } \
{sim:/tb_usb_shift/dut/rcv_data_0_port } \
{sim:/tb_usb_shift/dut/state_1_port } \
{sim:/tb_usb_shift/dut/state_0_port } \
{sim:/tb_usb_shift/dut/nextstate_1_port } \
{sim:/tb_usb_shift/dut/nextstate_0_port } \
{sim:/tb_usb_shift/dut/current_7_port } \
{sim:/tb_usb_shift/dut/current_6_port } \
{sim:/tb_usb_shift/dut/current_5_port } \
{sim:/tb_usb_shift/dut/current_4_port } \
{sim:/tb_usb_shift/dut/current_3_port } \
{sim:/tb_usb_shift/dut/current_2_port } \
{sim:/tb_usb_shift/dut/current_1_port } \
{sim:/tb_usb_shift/dut/current_0_port } 

run 2000 ns
